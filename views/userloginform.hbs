<h1>Log in</h1>
<div class="row">
  <div class="col-xs-12 col-sm-8">
    <p>Use your credentials below to log in.</p>
  </div>
</div>

<div class="row">
  <div class="col-xs-12 col-sm-8">
    <form id="userLogInForm" method="post">

      <div class="form-group row">
        <label for="email" class="form-control-label col-xs-12">Email*</label>
        <div class="col-xs-12">
          <input type="email" class="form-control" name="email" id="email" />
        </div>
      </div>

      <div class="form-group row">
        <label for="password" class="form-control-label col-xs-12">Password*</label>
        <div class="col-xs-12">
          <input type="password" class="form-control" name="password" id="password" />
        </div>
      </div>

      <div class="form-group row">
        <div class="col-xs-12 col-sm-5">
          <button type="submit" class="btn btn-default btn-block">Log in</button>
        </div>
      </div>
    </form>
  </div>
</div>
